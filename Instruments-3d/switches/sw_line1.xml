<animation>
	<type>pick</type>
	<object-name>hs1</object-name>
	<visible>false</visible>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>
				interpolate("an2/controls/switches/generator", 1.0, 0.1 );
			</script>
		</binding>
	</action>
</animation>

