<!-- Switch line
3D instrument for An-2-->
<PropertyList>
 <path>sw_line1.ac</path>
<!-- External instruments lighting -->
<animation>
  <type>material</type>
  <object-name>text</object-name>
  <object-name>base</object-name>
  <object-name>base.001</object-name>
  <object-name>base.002</object-name>
  <object-name>base.003</object-name>
  <object-name>base.004</object-name>
  <object-name>base.005</object-name>
  <object-name>base.006</object-name>
  <object-name>base.007</object-name>
  <object-name>hand</object-name>
  <object-name>hand.003</object-name>
  <object-name>hand.004</object-name>
  <object-name>hand.005</object-name>
  <object-name>hand.006</object-name>
  <object-name>hand.007</object-name>
  <object-name>hand.008</object-name>
  <object-name>hand.009</object-name>
  <emission>
    <red-prop>an2/controls/light/ext-red</red-prop>
    <green-prop>an2/controls/light/ext-green</green-prop>
    <blue-prop>an2/controls/light/ext-blue</blue-prop>
  </emission>
</animation>

<!--UV staff-->
<animation>
  <type>material</type>
  <object-name>spot</object-name>
  <object-name>spot.001</object-name>
  <object-name>spot.002</object-name>
  <object-name>spot.003</object-name>
  <object-name>spot.004</object-name>
  <object-name>spot.005</object-name>
  <object-name>spot.006</object-name>
  <object-name>spot.007</object-name>
 <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
    <factor-prop>an2/controls/light/uv-bright</factor-prop>
  </emission>
</animation>

<!--Handles-->
<animation>
    <type>rotate</type>
    <object-name>hand</object-name>
    <object-name>spot</object-name>
    <property>an2/controls/switches/generator</property>
    <offset-deg>30.0</offset-deg>
    <factor>-60.0</factor>
<!--    <center>
      <x-m>0.002</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
    </center>-->
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
</animation>

